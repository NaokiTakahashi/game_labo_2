function _init()
  poke(0x5f5c, 255)
  message.init()
  fader.init()
end

function _update()
  if co == nil then
    co = cocreate(game_main)
    coresume(co)
  end
  fader.update()
  message.update()
  ws.update()
end

function _draw()
  cls()
  if event_spn~=nil then
    local sx = (event_spn % 16) * 8
    local sy = flr(event_spn / 16) * 8
    sspr(sx,sy,64,48,0,0,128,96)
  end
  if bg_spn~=nil then
    local sx = (bg_spn % 16) * 8
    local sy = flr(bg_spn / 16) * 8
    sspr(sx,sy,64,16,0,24,128,32)
  end
  if l_spn~=nil then
    local sx = (l_spn % 16) * 8
    local sy = flr(l_spn / 16) * 8
    sspr(sx,sy,16,32,24,24,32,64,true)
  end
  if c_spn~=nil then
    local sx = (c_spn % 16) * 8
    local sy = flr(c_spn / 16) * 8
