    print("‚óÜ",116,116,7)
  end
end

function p(tbl)
  message.print(tbl,
  function()
    sfx(0)
    coresume(co)
  end)
  yield()
end

function s(tbl)
  ws.push{x=40,y=74,w=48,h=4+5*#tbl,
    texts=tbl,
    callback=function(i)
      sfx(0)
      ws.pop()
      coresume(co,i)
    end
  }
  return yield()  
end

function e(spn)
  fout()
  event_spn=spn
  bg_spn=nil
  l_spn=nil
  c_spn=nil
  r_spn=nil
  fin()
end

function b(spn)
  fout()
  bg_spn = spn
  event_spn=nil
  l_spn=nil
  r_spn=nil
  c_spn=nil
end

function cl(spn)
  l_spn = spn
end

function cc(spn)
  c_spn = spn
end

function cr(spn)
  r_spn = spn
end

function fader.init()
  fader.is_in=false
  fader.count=0
  fader.ptn=0
end

function fout()
  fader.is_in=false
  fader.count=16
  yield()
end

function fin()
  fader.is_in=true
  fader.count=16
  yield()
end

function fader.update()
  if fader.count>0 then
    local v
    if fader.is_in then
      v = fader.count
    else
      v = 16-fader.count
    end
    fader.set(v)
    fader.count-=1
    if (fader.count<=0) coresume(co)
  end
end

function fader.set(v)
  v=flr(v/4)
  if v==0 then
    fader.ptn=0xfffe + 0b0.1
  elseif v==1 then
    fader.ptn=0xfff6 + 0b0.1
  elseif v==2 then
    fader.ptn=0xfeb6 + 0b0.1
  elseif v==3 then
    fader.ptn=0xadb6 + 0b0.1
  else
    fader.ptn=0
  end
end

function fader.draw()
  if (fader.count==0) return
  fillp(fader.ptn)
  rectfill(0,0,127,127,0)
  fillp()
end

